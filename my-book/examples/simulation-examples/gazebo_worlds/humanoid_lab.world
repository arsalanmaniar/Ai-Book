              <size>0.3 0.3 0.5</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.3 1.0 0.3 1</diffuse>
            <specular>0.5 0.5 0.5 1</specular>
          </material>
        </visual>
        <inertial>
          <mass>6.0</mass>
          <inertia>
            <ixx>0.075</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.075</iyy>
            <iyz>0</iyz>
