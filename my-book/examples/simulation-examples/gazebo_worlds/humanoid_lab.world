        </inertial>
      </link>
      <static>1</static>
    </model>

    <!-- Stairs for locomotion testing -->
    <model name="stairs">
      <pose>3 2 0 0 0 0</pose>
      <link name="step1">
        <collision name="collision">
          <geometry>
            <box>
              <size>0.4 0.6 0.1</size>
